{
  "name": "react-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "axios": "^0.18.0",
    "bulma": "^0.7.1",
    "bulma-extensions": "^3.0.0",
    "classnames": "^2.2.6",
    "dotenv": "^6.0.0",
    "i18n-js": "^3.0.11",
    "markdown-to-jsx": "^6.7.3",
    "node-sass-chokidar": "^1.3.3",
    "react": "^16.5.1",
    "react-bulma-components": "^2.0.0",
    "react-dom": "^16.5.1",
    "react-markdown": "^4.0.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-s3-uploader": "^4.8.0",
    "react-scripts": "1.1.5",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "yarn build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "yarn watch-css & yarn start-js",
    "build-js": "react-scripts build",
    "build": "yarn build-css && yarn build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:5000"
}
